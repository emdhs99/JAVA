package javafestival3;

import java.util.Scanner;

public class ex07큰정수N값구하기 {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);

		System.out.print("입력 : ");
		int num1 = sc.nextInt();

		int num2 = 1;
		for (int i = 1; i <= num1; i++) {
			num2 = num2 * i;
		}
		System.out.print("출력 : " + num2);

	}

}