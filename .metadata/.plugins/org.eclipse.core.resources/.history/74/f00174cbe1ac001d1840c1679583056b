package for문;

public class ex06for문예제bonus {

	public static void main(String[] args) {

		
		int num1 = 0; 
		int num2 = 78; 
		
		for(int i = 1; i <= num2; i++) {
			
			num1 += (num2 - i) * i;
		 System.out.println(num1 + " + ");
		}
		System.out.println(num1);
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
