package 이중for문;

public class ex03별찍기 {

	public static void main(String[] args) {
		
		for (int j = 0; j < 5; j++) {
			for (int i = 5; i > 0; i--) {
				if(i > j) {
				System.out.print(" ");
				}else if (j > i) {
					System.out.println("*");
			}
		}
		}
		
//		System.out.print(" ");
