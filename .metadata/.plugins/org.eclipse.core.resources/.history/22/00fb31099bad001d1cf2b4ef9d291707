package 이중for문;

public class ex03별찍기 {

	public static void main(String[] args) {
		
		for (int j = 1; j > i; j++) {
			for (int i = 5; i > j; i--) {
				if(i > j) {
				System.out.print(" ");
				}
			} else if(j > i) {
			System.out.println("*");
			}
		}
		
		System.out.print(" ");
		System.out.print(" ");
		System.out.print(" ");
		System.out.print(" ");
		System.out.print("*");
		System.out.println();
		System.out.print(" ");
		System.out.print(" ");
		System.out.print(" ");
		System.out.print("*");
		System.out.print("*");
		System.out.println();
		System.out.print(" ");
		System.out.print(" ");
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.println();
		System.out.print(" ");
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.println();
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		System.out.print("*");
		
	}

}

