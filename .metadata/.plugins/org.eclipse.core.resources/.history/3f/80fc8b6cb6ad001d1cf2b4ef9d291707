package javafestival4;

public class ex03별찍기 {

	public static void main(String[] args) {
		for (int j = 1; j < 6; j++) {

			for (int t = 5; t > 0; t--) {
				if (t > j) {
					System.out.print(" ");
				} else {
					System.out.print("*");
				}
			}
			for (int i = 1; i < 5; i++) {
				if (i < j) {
					System.out.print("*");
				} else {
					System.out.print(" ");
				}
			}
			System.out.println();
		}
		
	}

}
