package 이중for문;

public class ex02별찍기 {

	public static void main(String[] args) {

		int num = 0;
		for (int j = 0; j < 5; j++) {

			for (int i = 0; i < num; i++) {
				System.out.print("*");
			}
			System.out.println("*");
			num ++;
		}
		
		System.out.println();
		
		for (int j = 0; j < 5; j++) {

			for (int i = 5; i > num; i--) {
				System.out.print("*");
			}
			System.out.println("");
			num ++;
		}


//		System.out.println("*");
//		System.out.print("*");
//		System.out.println("*");
//		System.out.print("*");
//		System.out.print("*");
//		System.out.println("*");
//		System.out.print("*");
//		System.out.print("*");
//		System.out.print("*");
//		System.out.println("*");
//		System.out.print("*");
//		System.out.print("*");
//		System.out.print("*");
//		System.out.print("*");
//		System.out.println("*");
	}

}
