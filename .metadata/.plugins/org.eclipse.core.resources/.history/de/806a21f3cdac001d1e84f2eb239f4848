package for문;

import java.util.Scanner;

public class ex02for문예제 {

	public static void main(String[] args) {
		
		
		for (int i = 96; i>72; i--) {
			System.out.print(i + " ");
		}
		
		for (int i = 96; i>72; i--) {
			if(i % 2 == 1 ) {
			System.out.print(i + " ");
			}
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}

}
